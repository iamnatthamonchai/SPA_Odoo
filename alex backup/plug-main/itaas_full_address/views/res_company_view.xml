<odoo>

    <record id="view_company_form_fax" model="ir.ui.view">
        <field name="name">res.company.form.fax</field>
        <field name="model">res.company</field>
        <field name="inherit_id" ref="base.view_company_form"/>
        <field name="arch" type="xml">

            <xpath expr="//sheet/div" position="inside">
                <label for="th_name" class="oe_edit_only"/>
                <h1>
                    <field name="th_name"/>
                </h1>
            </xpath>

            <xpath expr="//notebook/page/group/group[2]" position="inside">
                 <field name="fax"/>
				 <field name="th_name"/>
				 <field name="th_street"/>
				 <field name="th_street2"/>
				 <field name="th_city"/>
				 <field name="th_state_id"/>
                 <field name="th_country_id"/>
            </xpath>

        </field>
    </record>

</odoo>