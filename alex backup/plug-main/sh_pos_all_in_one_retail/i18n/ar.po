"                    <strong>REFERENCES</strong>\n"
"                    <br/>\n"
"                    Order number: <strong><t t-out=\"object.name or &quot;&quot;\"/></strong>\n"
"                    <br/>\n"
"                    Order total: <strong> <t t-out=\"object.amount_total or &quot;&quot;\"/>  <t t-out=\"object.pricelist_id.currency_id.name or &quot;&quot;\"/> </strong>\n"
"                    <br/>\n"
"                    Order date: <t t-out=\"object.date_order or &quot;&quot;\"/>  \n"
"                    <br/>\n"
"                    <t t-if=\"object.pos_reference\"> \n"
"                        Order Reference: <t t-out=\"object.pos_reference\"/>\n"
"                    </t>\n"
"                    <br/>\n"
"                    \n"
"                    Your contact: <a href=\"mailto:{{object.user_id.email or ''}}?subject=Order%20{{object.name}}\">\n"
"                        <t t-out=\"object.user_id.name or &quot;&quot;\"/></a>\n"
"                </p>\n"
"                <br/>\n"
