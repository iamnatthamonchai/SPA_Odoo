"                    </t>\n"
"                    <t t-if=\"object.company_id.website\">\n"
"                        <div> Web :<a href=\"{{object.company_id.website}}\"><t t-out=\"object.company_id.website\"/></a> </div> \n"
"                    </t>\n"
"                </div> \n"
"            </div> \n"
