<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">
    <t t-name="bag_qty_button" owl="1">
        <div class="control-button" t-on-click="trigger('click-bag_qty-button')">
            <div style="display: flex;align-items: center;">
	            <div class="sh_icon_img" style="margin-right: 6px;">
	            	<svg id="Layer_1" data-name="Layer 1" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 108.5 119.53"><path d="M114,50H102V38c-1-28-26-27-26-27a10.18,10.18,0,0,1,7,7c13,4,12,20,12,20V50H85V36C84,8,59,10,59,10,35,11,35,37,35,37V50H22l-6.5,79.5L124,129ZM42,36c2-20,18-19,18-19,17,1,18,18,18,18V50H59c-4-23,8-28,8-28-5-3-9-1-9-1-10,6-6,29-6,29H42Zm48,86H24l4-65H95Z" transform="translate(-15.5 -9.97)"></path></svg>
